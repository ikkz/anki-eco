{
  "extends": ["../../tsconfig.base.json"],
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "dist-types",
    "tsBuildInfoFile": "dist-types/tsconfig.lib.tsbuildinfo",
    "emitDeclarationOnly": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": ["./src/*"]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "module": "es2022",
    "moduleResolution": "bundler"
  },
  "include": ["src", "build/entries.ts"],
  "references": [
    {
      "path": "../../packages/analytics/tsconfig.lib.json"
    }
  ]
}
