{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "package": {
      "cache": true
    },
    "output": {
      "cache": true,
      "dependsOn": ["package"],
      "executor": "nx:run-commands",
      "options": {
        "commands": ["cp package.json output/package.json"],
        "cwd": "{projectRoot}"
      }
    }
  }
}
